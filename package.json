{
  "name": "node-libsamplerate",
  "version": "1.0.0",
  "description": "Native bindings for libsamplerate",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "libsamplerate",
    "samplerate",
    "audio",
    "sample",
    "rate",
    "convert",
    "resample",
    "downsample",
    "upsample",
    "n-api",
    "node-addon-api",
    "cmake-js",
    "S16_LE",
    "S32_LE",
    "pcm",
    "wav"
  ],
  "scripts": {
    "install": "mkdir deps/libsamplerate/build && cd deps/libsamplerate/build && cmake .. && make && cd ../../.. && node-gyp configure build",
    "build:dev": "node-gyp configure build --debug"
  },
  "dependencies": {
    "node-addon-api": "^1.4.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": "github:dmooney65/node-libsamplerate",
  "bugs": {
    "url": "https://github.com/dmooney65/node-libsamplerate/issues"
  },
  "homepage": "https://github.com/dmooney65/node-libsamplerate#readme"
}
